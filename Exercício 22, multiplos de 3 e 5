#include <stdio.h>

int main() {
    int num;
    
    printf("Digite um número inteiro: \n");
    scanf("%d", &num);
    
    printf("Estes são os múltiplos de 3 e 5 de 0 até %d:\n", num);
    
    for (int i = 0; i <= num; i++) {
        if (i % 3 == 0 || i % 5 == 0) { 
            printf("%d ", i);
        }
    }
    
    return 0;
}
